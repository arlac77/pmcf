{
  "extends": ["/services/systemd-journal-remote", "/services/bind"],
  "priority": 390,
  "routerId": 77,
  "masters": "/L1/host1/eth0",
  "backups": "/L1/n1/host2/wlan0",
  "authentication": {
    "type": "PASS",
    "password": "pass1234"
  },
  "networkInterfaces": {
    "eth0": {
      "kind": "ethernet",
      "network": "/L1/n1",
      "ipAddresses": "192.168.1.11"
    }
  },
  "services": {
    "bind": {
      "source": ["/GLOBAL", "/L1/n2"],
      "groups": {
        "internal": {
          "entries": "/L1/n1",
          "recordTTL": "1W",
          "access": "/L1/n1",
          "allowedUpdates": "trusted"
        },
        "protected": {
          "access": "/L1/n1",
          "sharedWith": "internal"
        }
      }
    },
    "ntp": {
      "source": "/GLOBAL"
    },
    "systemd-timesyncd": {
      "source": "/GLOBAL"
    },
    "systemd-resolved": {
      "source": "/GLOBAL",
      "LLMNR": true
    },
    "systemd-journald": {
      "SplitMode": "host"
    },
    "systemd-journal-upload": {
      "URL": "https://journal.examle.com/",
      "ServerKeyFile": "/etc/ssl/${domainName}.key"
    },
    "systemd-journal-remote": {
      "ServerKeyFile": "/etc/ssl/server.key"
    }
  }
}
