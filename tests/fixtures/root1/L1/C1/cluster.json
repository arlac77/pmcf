{
  "priority": 1,
  "routerId": 77,
  "masters": "/L1/host1/eth0",
  "backups": "/L1/n1/host2/wlan0",
  "authentication": {
    "type": "PASS",
    "password": "pass1234"
  },
  "networkInterfaces": {
    "eth0": {
      "kind": "ethernet",
      "network": "/L1/n1",
      "ipAddresses": "192.168.1.11"
    }
  },
  "services": {
    "bind": {
      "source": "/GLOBAL",
      "protected": "/L1/n1",
      "trusted": ["/LOCAL", "/L1/n1"],
      "allowedUpdates": ["trusted"],
      "recordTTL": "1W"
    },
    "ntp": {
      "source": "/GLOBAL"
    },
    "systemd-timesyncd": {
      "source": "/GLOBAL"
    },
    "systemd-resolved": {
      "source": "/GLOBAL"
    },
    "systemd-journal": {}
  }
}
