{
  "priority": 11,
  "os": "linux",
  "depends": ["d1", "d2-${os}"],
  "replaces": ["r1", "r2-${os}"],
  "provides": ["p1", "p2-${os}"],
  "extends": ["/model/m1", "/model/${os}"],
  "networkInterfaces": {
    "eth0": {
      "kind": "ethernet",
      "network": "/L1/n1",
      "ipAddresses": "192.168.1.1"
    }
  },
  "services": {
    "dns": { "type": "dns", "alias": "dns" },
    "openldap": {
      "baseDN": "${ldap_base}",
      "rootDN": "dn=root,${ldap_base}",
      "uri": "ldap://"
    },
    "chrony": {
      "source": "/GLOBAL"
    }
  }
}
