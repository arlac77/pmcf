{
  "os": "linux",
  "depends": ["d1", "d2"],
  "replaces": ["r1", "r2"],
  "provides": ["p1", "p2"],
  "extends": ["/model/m1", "/model/${os}"],
  "networkInterfaces": {
    "eth0": {
      "network": "/L1/n1",
      "ipAddresses": "192.168.1.1"
    }
  },
  "services": {
    "dns": { "type": "dns", "alias": "dns" }
  }
}
