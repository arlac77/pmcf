{
  "priority": 11,
  "os": "linux",
  "depends": ["d1", "d2-${os}"],
  "replaces": ["r1", "r2-${os}"],
  "provides": ["p1", "p2-${os}"],
  "extends": [
    "/model/m1",
    "/model/${os}",
    "/services/timemachine",
    "/services/mosquitto"
  ],
  "networkInterfaces": {
    "eth0": {
      "kind": "ethernet",
      "network": "/L1/n1",
      "ipAddresses": "192.168.1.1"
    }
  },
  "services": {
    "influxdb": {},
    "mosquitto": {
      "acl_file": "/etc/mosquitto/acl"
    },
    "dns": { "type": "dns", "alias": "dns" },
    "openldap": {
      "alias": "ldap",
      "baseDN": "${ldap_base}",
      "rootDN": "dn=root,${ldap_base}",
      "uri": "ldap://",
      "DB_CONFIG": {
        "set_cachesize": "0 16777216 1",
        "set_lg_regionmax": "65536",
        "set_lg_bsize": "524288"
      }
    },
    "chrony": {
      "alias": "ntp",
      "source": "/GLOBAL"
    }
  }
}
